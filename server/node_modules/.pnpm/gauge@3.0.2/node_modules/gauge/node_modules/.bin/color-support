#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/color-support@1.1.3/node_modules/color-support/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/color-support@1.1.3/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules:/home/matias/proyectos/chat/node_modules:/home/matias/proyectos/node_modules:/home/matias/node_modules:/home/node_modules:/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/gauge@3.0.2/node_modules/gauge/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/gauge@3.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/color-support@1.1.3/node_modules/color-support/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/color-support@1.1.3/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules:/home/matias/proyectos/chat/node_modules:/home/matias/proyectos/node_modules:/home/matias/node_modules:/home/node_modules:/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/gauge@3.0.2/node_modules/gauge/node_modules:/home/matias/proyectos/chat/chatFlat/node_modules/.pnpm/gauge@3.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../color-support@1.1.3/node_modules/color-support/bin.js" "$@"
else
  exec node  "$basedir/../../../../../color-support@1.1.3/node_modules/color-support/bin.js" "$@"
fi
